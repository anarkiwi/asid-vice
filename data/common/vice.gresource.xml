<?xml version="1.0" encoding="UTF-8"?>
<!-- When compiling with glib-compile-resources, the `sourcedir` argument is
     $(top_srcdir).
-->
<gresources>
  <gresource prefix="/org/pokefinder/vice">
    <file alias="vice-logo-black.svg">data/common/vice-logo-black.svg</file>
    <file alias="CBM_Logo.svg">data/common/CBM_Logo.svg</file>
    <file alias="Icon-128@2x.png">data/common/Icon-128@2x.png</file>
    <!-- emu icons, aliases are supposed to match `machine_name`, excl. 'svg'
         currently xcbm5x0 uses the 'CBM-II' icon -->
    <file alias="C64.svg">data/common/C64_1024.svg</file>
    <file alias="C64SC.svg">data/common/C64_1024.svg</file>
    <file alias="C64DTV.svg">data/common/DTV_1024.svg</file>
    <file alias="SCPU64.svg">data/common/SCPU_1024.svg</file>
    <file alias="C128.svg">data/common/C128_1024.svg</file>
    <file alias="PLUS4.svg">data/common/Plus4_1024.svg</file>
    <file alias="CBM-II.svg">data/common/CBM2_1024.svg</file>
    <file alias="VIC20.svg">data/common/VIC20_1024.svg</file>
    <file alias="PET.svg">data/common/PET_256.svg</file> <!-- probably should be 1024 -->
    <file alias="SID.svg">data/common/SID_1024.svg</file>
  </gresource>
</gresources>

